{
  "name": "chrome-web-store-item-property",
  "description": "Gather meta information from chrome web store",
  "version": "1.2.0",
  "author": {
    "name": "sanemat",
    "email": "o.gata.ken@gmail.com",
    "url": "sane.jp"
  },
  "dependencies": {
    "array-includes": "^3.0.3",
